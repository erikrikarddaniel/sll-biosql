<h1>Listing protein_gi_taxons</h1>

<table>
  <tr>
    <th>Protein gi</th>
    <th>Taxon</th>
    <th></th>
  </tr>

<% @protein_gi_taxons.each do |protein_gi_taxon| %>
  <tr>
    <td><%= protein_gi_taxon.protein_gi %></td>
    <td><%= link_to protein_gi_taxon.taxon_with_name.scientific_name, protein_gi_taxon.taxon_with_name %></td>
    <td><%= link_to 'Show', protein_gi_taxon %></td>
  </tr>
<% end %>
</table>

<br />
